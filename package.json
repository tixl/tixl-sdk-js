{
  "name": "@tixl/tixl-sdk-js",
  "version": "0.7.10",
  "scripts": {
    "prepublish": "yarn build",
    "build": "rm -rf dist && yarn build:js && yarn build:package && yarn build:worker",
    "build:js": "tsc",
    "build:package": "cp src/_package.json dist/package.json && touch dist/.npmignore && cp .yarnrc dist/",
    "build:worker": "npx webpack --config web-worker/webpack.config.js"
  },
  "files": [
    "dist/package.json",
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "dependencies": {
    "@bitauth/libauth": "1.17.3",
    "axios": "0.21.1",
    "bs58": "4.0.1",
    "buffer": "6.0.3",
    "build-query-string": "1.2.1",
    "expo-random": "10.0.0",
    "hash-emoji": "0.0.4",
    "js-sha256": "0.9.0",
    "jsbi": "3.1.4",
    "lodash": "4.17.20",
    "randombytes": "2.1.0",
    "react-redux": "7.2.2",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@types/aes-js": "3.1.1",
    "@types/base64-js": "1.3.0",
    "@types/bs58": "4.0.1",
    "@types/debug": "4.1.5",
    "@types/lodash": "4.14.167",
    "@types/node": "14.14.20",
    "@types/randombytes": "2.0.0",
    "@types/react": "17.0.0",
    "@types/react-redux": "7.1.15",
    "@types/secp256k1": "4.0.1",
    "debug": "4.3.1",
    "prettier": "2.2.1",
    "ts-loader": "8.0.14",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "webpack": "5.13.0",
    "webpack-cli": "4.3.1"
  },
  "peerDependencies": {
    "@tixl/tixl-types": "0.7.3"
  }
}
